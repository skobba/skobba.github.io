---
const { title = "Default Title", children } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{title}</title>
</head>
<body>
  <header>
    <h1>{title}</h1>
  </header>
  <main>
    <slot />
  </main>
  <footer>
    <p>Footer content here</p>
  </footer>
</body>
</html>
